{
  "name": "node-ui5",
  "version": "3.0.0-alpha",
  "description": "A NodeJS wrapper on OpenUI5 to leverage tools like MockServer or ODataModel",
  "main": "./index.js",
  "files": [
    "lib/*",
    "src/*",
    "*.js"
  ],
  "scripts": {
    "test": "standard && node tests/all.js && nyc mocha",
    "lint": "standard --fix",
    "mocha": "mocha",
    "cover": "nyc mocha && nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArnaudBuchholz/node-ui5.git"
  },
  "keywords": [
    "openui5",
    "odata",
    "mock",
    "server"
  ],
  "author": "Arnaud Buchholz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArnaudBuchholz/node-ui5/issues"
  },
  "homepage": "https://github.com/ArnaudBuchholz/node-ui5#readme",
  "dependencies": {
    "colors": "^1.3.3",
    "deasync": "^0.1.15",
    "gpf-js": "^1.0.0",
    "mime": "^2.4.4",
    "reserve": "^1.1.1"
  },
  "devDependencies": {
    "@openui5/sap.ui.core": "^1.71.3",
    "eslint-utils": ">=1.4.3",
    "js-yaml": "^3.13.1",
    "jsdom": "^15.1.1",
    "lodash": ">=4.17.13",
    "mocha": "^10.2.0",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.1",
    "standard": "^14.3.1"
  },
  "standard": {
    "globals": [
      "process",
      "describe",
      "it",
      "before",
      "after"
    ]
  },
  "mocha": {
    "spec": [
      "tests/mocha/*.test.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "exclude": [],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  }
}
